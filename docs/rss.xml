#include&lt;iostream&gt;

using namespace std;
int n;
int f(int k){
	if(!k){
		return 1;
	}
	return k*f(k-1);
}
int main(){
	ios::sync_with_stdio(0);
	cin.tie(nullptr);
	cin&gt;&gt;n;
	cout&lt;&lt;f(n);
	return 0;
}
